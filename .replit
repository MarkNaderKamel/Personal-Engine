modules = ["php-8.2", "web", "nodejs-20"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["postgresql"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Life Atlas Server"

[[workflows.workflow]]
name = "Life Atlas Server"
author = "agent"

[[workflows.workflow.tasks]]
